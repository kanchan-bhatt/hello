						<tr>
							<td valign="top">First name:
							</td>
							<td>	<strong>-</strong></td>
						</tr>
						<tr>
							<td valign="top">Last name:</td>
						</tr>
						<tr>
							<td valign="top">Email:</td>
						</tr>
						<tr>
							<td valign="top">Phone</td>
						</tr>
					</tbody>
				</table>
	</div>
</div>

